#include <stdio.h>

int main() {
    int n, frames, i, j, k, pageFaults=0;
    printf("Enter number of pages: "); scanf("%d",&n);
    int pages[n];
    printf("Enter page reference sequence: ");
    for(i=0;i<n;i++) scanf("%d",&pages[i]);
    printf("Enter number of frames: "); scanf("%d",&frames);

    int f[frames], time[frames];
    for(i=0;i<frames;i++) f[i]=-1, time[i]=0;

    for(i=0;i<n;i++){
        int flag=0, lru=0;
        for(j=0;j<frames;j++) if(f[j]==pages[i]) {flag=1; time[j]=i; break;}
        if(flag==0){
            int min=time[0], pos=0;
            for(j=0;j<frames;j++) if(time[j]<min){min=time[j]; pos=j;}
            f[pos]=pages[i]; time[pos]=i; pageFaults++;
        }
        printf("Frames: ");
        for(j=0;j<frames;j++) printf("%d ", f[j]);
        printf("\n");
    }
    printf("Total page faults: %d\n", pageFaults);
    return 0;
}
