#include <stdio.h>

int main() {
    int n, frames, i, j, k, pageFaults=0;
    printf("Enter number of pages: "); scanf("%d",&n);
    int pages[n];
    printf("Enter page reference sequence: ");
    for(i=0;i<n;i++) scanf("%d",&pages[i]);
    printf("Enter number of frames: "); scanf("%d",&frames);

    int f[frames];
    for(i=0;i<frames;i++) f[i]=-1;

    for(i=0;i<n;i++){
        int flag=0;
        for(j=0;j<frames;j++) if(f[j]==pages[i]) {flag=1; break;}
        if(flag==0){
            int pos=-1, farthest=-1;
            for(j=0;j<frames;j++){
                int found=0;
                for(k=i+1;k<n;k++) if(f[j]==pages[k]) {found=k; break;}
                if(found>farthest || f[j]==-1){farthest=found; pos=j;}
            }
            f[pos]=pages[i];
            pageFaults++;
        }
        printf("Frames: ");
        for(j=0;j<frames;j++) printf("%d ", f[j]);
        printf("\n");
    }
    printf("Total page faults: %d\n", pageFaults);
    return 0;
}
